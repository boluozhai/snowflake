<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
 "http://www.springframework.org/dtd/spring-beans-2.0.dtd" >
<beans>


	<import resource="classpath:config/spring/snowflake-lib-xgit-vfs.xml" />
	<import resource="classpath:config/spring/snowflake-lib-xgit-command.xml" />


	<bean id='com.boluozhai.snowflake.appdata.AppDataAgent'
		class='com.boluozhai.snowflake.appdata.support.DefaultAppDataAgent' />

	<bean id='com.boluozhai.snowflake.cli.client.CLIClientFactory'
		class='com.boluozhai.snowflake.cli.support.DefaultCliClientFactory' />

	<bean id='com.boluozhai.snowflake.cli.service.CLIServiceFactory'
		class='com.boluozhai.snowflake.cli.support.DefaultCliServiceFactory' />

	<bean id='com.boluozhai.snowflake.xgit.utils.RepositoryAgent'
		class='com.boluozhai.snowflake.xgit.utils.DefaultRepositoryAgent' />

	<bean id='com.boluozhai.snowflake.xgit.utils.CurrentLocation'
		class='com.boluozhai.snowflake.xgit.utils.DefaultCurrentLocation' />

</beans>
